        ]
    },
    {
        "number": 17,
        "title": "git reset",
        "content": [
            {
                "id": "64336efefd905bada37c1a01c0b0ee47",
                "content": "This command unstages the file, but it preserves the file contents.",
                "code": "git reset [file]"
            },
            {
                "id": "85c61d15655753c0c1507967b3defd66",
                "content": "This command undoes the last commit and moves changes locally to staged.",
                "code": "git reset --soft HEAD~"
            },
            {
                "id": "c7ea9cae7e5e50e3e63b362a4ce2523e",
                "content": "This command discards the last commit and its contents.",
                "code": "git reset --hard HEAD~"
            }
        ]
    },
    {
